<%= form_with(model: current_user.bookmarks.find_by(technique_id: @technique.id), remote: true, method: :delete) do |f| %>
  <%= hidden_field_tag :technique_id, @technique.id %>
  <%= f.button :type => "submit", class: 'unfavorite-icon btn btn-lg btn-light' do %>
    <i class="fas fa-bookmark"></i>
  <% end %>
<% end %>